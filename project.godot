; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "ContainerObject",
"language": "GDScript",
"path": "res://scripts/interaction/container_obj.gd"
}, {
"base": "Resource",
"class": "InteractionObject",
"language": "GDScript",
"path": "res://scripts/interaction/interaction_object.gd"
}, {
"base": "Resource",
"class": "ItemObject",
"language": "GDScript",
"path": "res://scripts/interaction/item_obj.gd"
} ]
_global_script_class_icons={
"ContainerObject": "",
"InteractionObject": "",
"ItemObject": ""
}

[application]

config/name="WoF RPG"
run/main_scene="res://scenes/loading_screen.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="wof_rpg"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

gstate="*res://scripts/state.gd"
pstate="*res://scripts/player/playerstate.gd"

[display]

window/stretch/mode="2d"
window/stretch/aspect="expand"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[logging]

file_logging/enable_file_logging=true
file_logging/log_path="user://logs/wof_rpg.log"
file_logging/max_log_files=10

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
